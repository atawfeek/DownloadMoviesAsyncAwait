@model IEnumerable<IEnumerable<Movie>>

@{
    ViewData["Title"] = "Home Page";
}

<table>
    @foreach (var movies in Model)
    {
        <tr><td><b>Category: @movies.FirstOrDefault().Category</b></td></tr>
        <tr>
            @foreach (var movie in movies)
            {
                <td>
                    <div>@movie.Title</div>
                    <div><img src="@movie.ImageUrl" style="width: 185px;" /></div>
                </td>
            }
        </tr>
    }
</table>

<b>Execution time: @ViewBag.Elapsed</b>
